package com.qarea.mlfw.util;

import java.util.ArrayList;

public class WordValue {

	String word; // word
	String type; // type of word
	ArrayList<String> translation; // word translate

	public WordValue() {
		word = "";
		type = "";
		translation = new ArrayList<String>();
	}

	public void getTranslation(ArrayList<String> translation) {
		for (String tmp : this.translation) {
			translation.add(tmp);
		}
	}

	public void setTranslation(ArrayList<String> translation) {
		for (String tmp : translation) {
			this.translation.add(tmp);
		}
	}

	public String getWord() {
		return word;
	}

	public void setWord(String word) {
		if (word != "") {
			this.word = word;
		}
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

}
