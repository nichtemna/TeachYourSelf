package com.qarea.mlfw.util;

public class WordStatistic {

	private int wordId;
	private int dictonaryId;
	private String wordName;
	private double percent;
	private int answerCount;
	private int correctAnswerCount;
	private long date;

	public WordStatistic(int wordId, String wordName, double percent,
			int answerCount, int correctAnswerCount, long date, int dictonaryId) {
		this.wordId = wordId;
		this.wordName = wordName;
		this.percent = percent;
		this.answerCount = answerCount;
		this.correctAnswerCount = correctAnswerCount;
		this.date = date;
		this.dictonaryId = dictonaryId;
	}

	public int getWordId() {
		return wordId;
	}

	public String getWordName() {
		return wordName;
	}

	public double getPercent() {
		return percent;
	}

	public int getAnswerCount() {
		return answerCount;
	}

	public int getCorrectAnswerCount() {
		return correctAnswerCount;
	}

	public long getDate() {
		return date;
	}

	public int getDictonaryId() {
		return dictonaryId;
	}
	
	
}
