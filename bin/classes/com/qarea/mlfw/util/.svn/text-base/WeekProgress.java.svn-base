package com.qarea.mlfw.util;

public class WeekProgress {

	private String weekName;
	private int allAnswerCount;
	private int correctAnswerCount;
	public long weekFirstDay;
	
	public WeekProgress(String weekName, int allAnswerCount,
			int correctAnswerCount, long weekFirstDay) {
		this.weekName = weekName;
		this.allAnswerCount = allAnswerCount;
		this.correctAnswerCount = correctAnswerCount;
		this.weekFirstDay = weekFirstDay;
	}

	public String getWeekName() {
		return weekName;
	}

	public int getAllAnswerCount() {
		return allAnswerCount;
	}

	public int getCorrectAnswerCount() {
		return correctAnswerCount;
	}

	public long getWeekFirstDay() {
		return weekFirstDay;
	}

}
